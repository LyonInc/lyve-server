{
  "name": "chat",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuid"
    },
    "message": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "event": {
      "type": "belongsTo",
      "model": "event",
      "foreignKey": ""
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
