{
  "name": "event",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuid"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "number",
      "required": true
    },
    "timeStart": {
      "type": "date",
      "required": true
    },
    "timeEnd": {
      "type": "date",
      "required": true
    },
    "isLive": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "chatPath": {
      "type": "string",
      "required": true
    },
    "imgHeaderPath": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "channel": {
      "type": "belongsTo",
      "model": "channel",
      "foreignKey": ""
    },
    "categories": {
      "type": "hasMany",
      "model": "category",
      "foreignKey": "eventId",
      "through": "eventcategory"
    }
  },
  "acls": [],
  "methods": {}
}
