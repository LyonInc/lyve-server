{
  "name": "subscriber",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuid"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    },
    "channel": {
      "type": "belongsTo",
      "model": "channel",
      "foreignKey": "channelId"
    }
  },
  "acls": [],
  "methods": {}
}
